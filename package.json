{
  "name": "panza",
  "version": "2.2.2",
  "description": "a foundation for your react-native projects",
  "main": "lib/index.js",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "generate-doc-json": "react-docgen components/panza/**/* -o documentation/components.json --pretty",
    "generate-doc-markdown": "node scripts/generate-docs.js",
    "generate-docs": "npm run generate-doc-json && npm run generate-doc-markdown",
    "build": "rimraf lib && babel ./components/panza -d lib",
    "web-server": "NODE_ENV=development node -r ./web/register.js web/server/index.js",
    "web-build": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmcmahen/panza.git"
  },
  "keywords": [
    "react-native",
    "widgets",
    "ui",
    "bootstrap",
    "foundation",
    "forms"
  ],
  "author": "Ben McMahen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmcmahen/panza/issues"
  },
  "homepage": "https://github.com/bmcmahen/panza#readme",
  "dependencies": {
    "color": "^0.11.1",
    "react-native-vector-icons": "^2.0.3"
  },
  "peerDependencies": {
    "react-native-web": "0.0.25",
    "react-dom": "^15.1.0",
    "react-native": "^0.27.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "react": "^15.1.0",
    "react-docgen": "^2.8.2",
    "rimraf": "^2.5.2",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "cookie-parser": "^1.4.2",
    "cookie-session": "^1.2.0",
    "css-loader": "^0.23.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "invariant": "^2.2.1",
    "morgan": "^1.7.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-native-web": "0.0.25",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "throng": "^4.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
